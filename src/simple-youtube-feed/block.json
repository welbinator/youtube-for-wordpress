{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "yt-for-wp/simple-youtube-feed",
	"version": "0.1.0",
	"title": "Simple Youtube Feed",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"attributes": {
        "layout": {
            "type": "string",
            "default": "grid"
        },
		"maxVideos": {
            "type": "number",
            "default": 5
        },
		"selectedPlaylist": {
            "type": "string",
            "default": ""
        },
		"enableSearch": { 
			"type": "boolean", 
			"default": false 
		},
		"enablePlaylistFilter": {
            "type": "boolean",
            "default": false
        }
    },
	"textdomain": "yt-for-wp",
	"editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "viewScript": "file:./view.js"
}